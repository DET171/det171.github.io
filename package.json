		"astro-eslint-parser": "^1.0.2",
		"autoprefixer": "^10.4.20",
		"globals": "^15.9.0",
		"mdast-util-to-string": "^4.0.0",
		"pagefind": "^1.1.0",
		"postcss": "^8.4.43",
		"postcss-html": "^1.7.0",
		"postcss-import": "^16.1.0",
		"prettier": "^3.3.3",
		"prettier-plugin-astro": "^0.14.1",
		"prettier-plugin-tailwindcss": "^0.6.6",
		"reading-time": "^1.5.0",
		"remark-unwrap-images": "^4.0.0",
